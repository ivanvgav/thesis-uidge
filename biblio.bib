@article{bellucci2021,
  title = {An Analysis of {{Existential Graphs}}–Part 2: {{Beta}}},
  shorttitle = {An Analysis of {{Existential Graphs}}–Part 2},
  author = {Bellucci, Francesco and Pietarinen, Ahti-Veikko},
  year = {2021-12},
