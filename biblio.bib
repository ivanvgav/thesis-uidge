  doi = {10.1007/s11098-016-0667-6},
  url = {https://doi.org/10.1007/s11098-016-0667-6},
  urlyear = {2022-08-09},
  abstract = {It is well known that reductio ad absurdum arguments raise a number of interesting philosophical questions. What does it mean to assert something with the precise goal of then showing it to be false, i.e. because it leads to absurd conclusions? What kind of absurdity do we obtain? Moreover, in the mathematics education literature number of studies have shown that students find it difficult to truly comprehend the idea of reductio proofs, which indicates the cognitive complexity of these constructions. In this paper, I start by discussing four philosophical issues pertaining to reductio arguments. I then briefly present a dialogical conceptualization of deductive arguments, according to which such arguments (especially mathematical proofs) are best understood as a dialogue between two (fictitious) participants—Prover and Skeptic. Finally, I argue that many of the philosophical and cognitive difficulties surrounding reductio arguments are dispelled or at least further clarified once one adopts a dialogical perspective.},
  langid = {english},
  file = {/home/ivan/Documents/articles/dutilh_novaes2016-reductio_ad_absurdum_from_a_dialogical_perspective.pdf}
}

