@incollection{hintikka2004,
  title = {Logical vs. {{Nonlogical Concepts}}: {{An Untenable Dualism}}?},
  shorttitle = {Logical vs. {{Nonlogical Concepts}}},
  booktitle = {Logic, {{Epistemology}}, and the {{Unity}} of {{Science}}},
  author = {Hintikka, Jaakko},
  editor = {Rahman, Shahid and Symons, John and Gabbay, Dov M. and family=Bendegem, given=Jean Paul, prefix=van, useprefix=false},
  year = {2004},
  series = {Logic, {{Epistemology}}, {{And The Unity Of Science}}},
  pages = {51--56},
  publisher = {{Springer Netherlands}},
  location = {{Dordrecht}},
  doi = {10.1007/978-1-4020-2808-3_5},
  url = {https://doi.org/10.1007/978-1-4020-2808-3_5},
  urlyear = {2022-08-29},
  abstract = {One of the greatest disasters that befell twentieth-century analytic philosophy was Quine's (1953) rejection of the distinction between analytic and synthetic truths as an “untenable dualism”, to use Morton White's (1950) phrase. Or rather, the disaster was the widespread acceptance of this untenable rejection. It deprived philosophers of the means of mastering the defining concept of our era, the notion of information. It made it apparently pointless for them to use axiomatization or other kind of logical systematization as a tool of serious philosophical analysis, and hence encouraged the currently popular no-brainer appeals to “intuitions”. (cf. Hintikka 1999) These unfortunate consequences follow because on Quine's view a deduction of a theorem from axioms (and more generally a deduction of a consequence from premises) can introduce what for us is new factual information, for according to Quine such information cannot be separated from what for us is purely linguistic information. Hence the cognitive content (factual information) of a theory cannot be summed up in its axioms, for new assumptions can be introduced by the logical and mathematical methods used in the derivation of the theorems. This was precisely the kind of conundrum that the great David Hilbert sought to eliminate by means of his axiomatic approach, as shown by the sixth problem in his famous list of open problems. (See here e.g., Cory 1997, 1998; Majer 2001; and Yandell 2002)},
  isbn = {978-1-4020-2808-3},
  langid = {english}
}

