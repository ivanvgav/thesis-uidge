}

@article{dybowski2022,
  title = {Game Analogy in Law Reconsidered: Is Evidence at Stake?},
  shorttitle = {Game Analogy in Law Reconsidered},
  author = {Dybowski, Maciej and Dzięgielewska, Weronika and Rzepiński, Wojciech},
  year = {2022-12-10},
