@book{mancosu2021,
  title = {An {{Introduction}} to {{Proof Theory}}: {{Normalization}}, {{Cut-Elimination}}, and {{Consistency Proofs}}},
  shorttitle = {An {{Introduction}} to {{Proof Theory}}},
