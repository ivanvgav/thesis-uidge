@article{okwenna2021,
  title = {{{PEIRCE}}’{{S FALLIBILISM}}: {{A THEMATIC ANALYSIS AND THE REVISITATION OF THE ORIGINS OF FALLIBILISM}}},
  author = {OKWENNA, Chysogonus M.},
  year = {2021},
  journaltitle = {AMAMIHE: Journal of Applied Philosophy},
  volume = {19},
