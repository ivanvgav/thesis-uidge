}

@incollection{kiryushchenko2023a,
  title = {Existential {{Graphs}}},
  booktitle = {Diagrams, {{Visual Imagination}}, and {{Continuity}} in {{Peirce}}'s {{Philosophy}} of {{Mathematics}}},
  author = {Kiryushchenko, Vitaly},
  editor = {Kiryushchenko, Vitaly},
  year = {2023},
  series = {Mathematics in {{Mind}}},
