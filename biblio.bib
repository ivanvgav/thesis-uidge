@article{irikefe2022,
  title = {The Epistemology of Thought Experiments without Exceptionalist Ingredients},
  author = {Irikefe, Paul O.},
  year = {2022-04-28},
  journaltitle = {Synthese},
  shortjournal = {Synthese},
